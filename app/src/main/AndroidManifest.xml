<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.imran.totalityassignment">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TotalityAssignment"
        android:requestLegacyExternalStorage="true"
       >
        <activity android:name=".OpenCameraActivity"></activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".editimage.Tune_Activity" />
        <activity android:name=".editimage.Rotate_Crop_Activity" />
        <activity android:name=".editimage.Add_Text_Activity" />
        <activity android:name=".editimage.Image_Display_Activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>